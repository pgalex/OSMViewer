<?xml version="1.1" encoding="UTF-8" ?>

<!-- Description of all objects that can be on map (can be drawen on map) and how to draw them. Linked with map drawing procedure -->

<!-- Id of each map object must be unique. descriptionId linked with MapObjectsDescription -->

<mapObject drawinId="natural-wood" descriptionId = "wood"/>
	<drawingParameters type="polygon">
		<filling>
			<scale from="2" to="18">
				<color r="0" g="255" b="0" a="255"/>
			</scale>
		</filling>
		<text>
			<textTagKey>name</textTagKey>
			<textTagKey>description</textTagKey>

			<scale from="2" to="18">
				<color r="0" g="0" b="0" a="255"/>
				<font name="Arial" size="10" bold="false" italic="false" underline="false"/>
			</scale>
		</text>
	</<drawingParameters>
</mapObject>